<Bindings>
	<Binding name="AngrySparks_DISPLAY" header="AngrySparks" category="ADDONS">
		AngrySparks_ToggleDisplay()
	</Binding>
	<Binding name="AngrySparks_SHOW_DISPLAY" category="ADDONS">
		AngrySparks_ShowDisplay()
	</Binding>
	<Binding name="AngrySparks_HIDE_DISPLAY" category="ADDONS">
		AngrySparks_HideDisplay()
	</Binding>
	<Binding name="AngrySparks_LOCK" category="ADDONS">
		AngrySparks_ToggleLock()
	</Binding>
	<Binding name="AngrySparks_WINDOW" category="ADDONS">
		AngrySparks_ToggleWindow()
	</Binding>
	<Binding name="AngrySparks_OUTPUT" category="ADDONS">
		AngrySparks_OutputDisplayed()
	</Binding>
</Bindings>
